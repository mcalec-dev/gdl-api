{
  "name": "gdl-api",
  "version": "0.2.38",
  "description": "gdl-api express api server + frontend + bots",
  "author": "McAlec",
  "keywords": [
    "gdl-api",
    "api",
    "mcalec.dev"
  ],
  "license": "MIT",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "set DEBUG=gdl-api:* && nodemon .",
    "start": "set DEBUG=gdl-api:* && node server.js",
    "server": "set DEBUG=gdl-api:* && node server.js",
    "lint": "eslint .",
    "format": "prettier . --write",
    "minify": "set DEBUG=gdl-api:* && node -e 'import('./minify.js').processFiles'",
    "rollup": "set DEBUG=gdl-api:* && node rollup.js",
    "build": "npm run lint && npm run format && npm run minify"
  },
  "dependencies": {
    "@ruffle-rs/ruffle": "^0.2.0-nightly.2026.2.3",
    "bcrypt": "^6.0.0",
    "bytes": "^3.1.2",
    "chalk": "^4.1.2",
    "clean-css": "^5.3.3",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "debug": "^4.4.1",
    "dotenv": "^17.2.0",
    "ejs": "^4.0.1",
    "encodeurl": "^2.0.0",
    "eslint-plugin-jsonc": "^2.21.0",
    "express": "^4.21.2",
    "express-jwt": "^8.5.1",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.19.0",
    "express-useragent": "^2.1.0",
    "fast-glob": "^3.3.3",
    "figlet": "^1.9.4",
    "fs-extra": "^11.3.3",
    "isbot": "^5.1.33",
    "js-yaml": "^4.1.1",
    "lusca": "^1.7.0",
    "mammoth": "^1.11.0",
    "mime-types": "^3.0.2",
    "mongoose": "^9.1.5",
    "morgan-body": "^2.6.9",
    "ms": "^2.1.3",
    "multer": "^2.0.2",
    "normalize-url": "^8.1.1",
    "passport": "^0.7.0",
    "passport-discord-auth": "^1.2.0",
    "passport-github2": "^0.1.12",
    "passport-local": "^1.0.0",
    "sanitize-filename": "^1.6.3",
    "sharp": "^0.34.5",
    "slash": "^5.1.0",
    "swagger-parser": "^10.0.3",
    "swagger-ui-express": "^5.0.1",
    "terser": "^5.46.0",
    "uuid": "^13.0.0",
    "validator": "^13.15.26"
