{
  "plugins": ["prettier-plugin-tailwindcss"],
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": false,
  "useTabs": false,
  "singleQuote": true,
  "htmlWhitespaceSensitivity": "ignore",
  "overrides": [
    {
      "files": "*.ejs",
      "options": {
        "parser": "html",
        "printWidth": 100000
      }
    }
  ]
}
